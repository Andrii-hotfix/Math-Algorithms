#: import FadeTransition kivy.uix.screenmanager.FadeTransition

ScreenManagement:
	transition: FadeTransition()
	MainMenu:
	EulerFunction:
	
<MainMenu>:
	name: "main"
	#Button:
		#on_release: app.root.current = "euler_function"
		#text: "Euler Function"
		#size_hint: 0.3, 0.2
		#font_size: 0.3*self.height
		
		
<EulerFunction>:
	name: "euler_function"
	#TextInput:
		#text: "Input a number:"
		#multiline: False
		#size_hint: 0.3, 0.2
	#Button:
		#on_release: app.root.current = "euler_function_res"
		#text: "Input"
		#size_hint: 0.3, 0.2
		#pos_hint: {"right":1, "top":1}
		
